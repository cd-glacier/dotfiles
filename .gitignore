!mac/bin/*.sh
!mac/.gitconfig
!mac/.gitignore_global
!mac/.ideavimrc
!mac/.pryrc
!mac/.vimrc
!mac/.xvimrc
!mac/.myclirc
!mac/.pgclirc

!tmux/bin/*.sh
!tmux/.tmux.conf

!zellij/bin/*.sh
!zellij/.config/zellij/config.yaml

!tools/co
!tools/grpc-call
!tools/ghe-noti
!tools/memo
!tools/add
!tools/pr
!tools/mypr
!tools/issue
!tools/push
!tools/preview-git-file
!tools/studio
!tools/xcode
!tools/rubymine
!tools/idea
!tools/java-home
!tools/bin/*
!tools/completions/*.fish

!swiftbar/bin/*.sh
!swiftbar/.gitkeep
!swiftbar/ghe-notification.10s.sh

