## .gitignore ##
# ignore all
*
!*/

nvim/.config/dein/.cache/
nvim/.config/dein/repos/
nvim/.config/coc/extensions/

# allow
!Makefile
!README.md
!bin/*.sh

!nvim/bin/*.sh
!nvim/.config/coc/extensions/package.json
!nvim/.config/dein/.gitkeep
!nvim/.config/nvim/init.vim
!nvim/.config/nvim/coc-settings.json
!nvim/.config/nvim/dein.toml
!nvim/.config/nvim/lombok.jar
!nvim/.config/nvim/spell/en.utf-8.add

!vscode/bin/*.sh
!vscode/settings.json
!vscode/keybindings.json
!vscode/extensions.list

!fish/bin/*.sh
!fish/.config/fish/config.fish
!fish/.config/fish/functions/*.fish
fish/.config/fish/functions/nippo.fish
!fish/.config/fish/completions/*.fish
!fish/.config/fish/completions/.gitkeep
fish/.config/fish/completions/envchain.fish
fish/.config/fish/completions/cpad2.fish

!bash/bin/*.sh
!/bash/.bash_profile
!/bash/.bashrc

!mac/bin/*.sh
!mac/.bash_profile
!mac/.bashrc
!mac/.gitconfig
!mac/.gitignore_global
!mac/.ideavimrc
!mac/.pryrc
!mac/.vimrc
!mac/.xvimrc
!mac/.myclirc
!mac/.pgclirc

!tmux/bin/*.sh
!tmux/.tmux.conf

!tools/co
!tools/grpc-call
!tools/ghe-noti
!tools/memo
!tools/pr
!tools/push
!tools/studio
!tools/xcode
!tools/rubymine
!tools/bin/*
!tools/completions/*.fish

