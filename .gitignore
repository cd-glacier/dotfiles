# https://qiita.com/Hi-king/items/a8485557095692f1bf13
## .gitignore ##
# ignore all
*
!*/

nvim/.config/dein/.cache/
nvim/.config/dein/repos/

# allow
!Makefile
!README.md
!bin/*.sh

!nvim/bin/*.sh
!nvim/.config/dein/.gitkeep
!nvim/.config/nvim/init.vim
!nvim/.config/nvim/dein.toml
!nvim/.config/nvim/spell/en.utf-8.add

!vscode/bin/*.sh
!vscode/settings.json
!vscode/keybindings.json

!fish/bin/*.sh
!fish/.config/fish/config.fish
!fish/.config/fish/functions/*.fish
fish/.config/fish/functions/adlocal.fish
fish/.config/fish/functions/nippo.fish

!mac/bin/*.sh
!mac/.bash_profile
!mac/.bashrc
!mac/.gitconfig
!mac/.gitignore_global
!mac/.ideavimrc
!mac/.pryrc
!mac/.vimrc

!tmux/bin/*.sh
!tmux/.tmux.conf
