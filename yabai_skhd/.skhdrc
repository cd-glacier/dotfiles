# window 移動
ctrl - j : yabai -m window --focus $(yabai -m query --windows --space | jq -re 'map(select(."is-minimized"==false)) | sort_by(.id) as $list | ($list | map(."has-focus") | index(true) // 0) as $cur | $list[($cur - 1) % length].id')
ctrl - k : yabai -m window --focus $(yabai -m query --windows --space | jq -re 'map(select(."is-minimized"==false)) | sort_by(.id) as $list | ($list | map(."has-focus") | index(true) // 0) as $cur | $list[($cur + 1 + length) % length].id')

# 位置変更
alt + ctrl - h  : yabai -m window --swap west || yabai -m display --swap west
alt + ctrl - j  : yabai -m window --swap south || yabai -m display --swap south
alt + ctrl - k  : yabai -m window --swap north || yabai -m display --swap north
alt + ctrl - l  : yabai -m window --swap east || yabai -m display --swap east

